<h1>{{this.first_name}} {{this.last_name}}</h1>
<nav>
  <a class="link" href="/books">View all the books</a>
  <a class="link" href="/authors">View all the authors</a>
</nav>

<ul>
 {{#each books}}
  <div class="book">
    <li>Name: {{this.title}}</li>
    <li>Year Released: {{this.year}}</li>
    <li>Read it?: {{this.have_read}}</li>
    <li><a href="/books/{{this.id}}"><img src="http://covers.librarything.com/devkey/{{this.key}}/medium/isbn/{{this.isbn}}" /></a></li>
  </div>
 {{/each}}
</ul>